<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.vse.aplikace.MainController">

    <left>
        <VBox>
            <children>
                <Button fx:id="account" mnemonicParsing="false" prefHeight="40.0" prefWidth="80.0" text="Account">
                    <VBox.margin>
                        <Insets left="10.0" top="20.0" />
                    </VBox.margin></Button>
                <Button fx:id="add" mnemonicParsing="false" prefHeight="40.0" prefWidth="80.0" text="Add">
                    <VBox.margin>
                        <Insets left="10.0" top="20.0" />
                    </VBox.margin></Button>
                <Button fx:id="transaction" mnemonicParsing="false" prefHeight="40.0" prefWidth="80.0" text="Transaction">
                    <VBox.margin>
                        <Insets left="10.0" top="20.0" />
                    </VBox.margin></Button>
                <Button fx:id="overview" mnemonicParsing="false" prefHeight="40.0" prefWidth="80.0" text="Overview">
                    <VBox.margin>
                        <Insets bottom="20.0" left="10.0" top="20.0" />
                    </VBox.margin></Button>
                <ImageView fitHeight="100" fitWidth="100" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@logo.png" />
                    </image>
                    <VBox.margin>
                        <Insets bottom="20.0" top="20.0" />
                    </VBox.margin>
                </ImageView>
            </children>
        </VBox>
    </left>

    <center>
        <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="69.0" prefWidth="500.0">
               <children>
                  <Button fx:id="addTransaction" mnemonicParsing="false" prefHeight="30.0" prefWidth="105.0" text="Transaction">
                     <HBox.margin>
                        <Insets bottom="15.0" left="35.0" right="15.0" top="10.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="2.0" left="5.0" />
                     </padding>
                  </Button>
                  <Button fx:id="addLoan" mnemonicParsing="false" prefHeight="30.0" prefWidth="105.0" text="Loan">
                     <HBox.margin>
                        <Insets bottom="15.0" left="40.0" right="15.0" top="10.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="2.0" left="5.0" />
                     </padding>
                  </Button>
                  <Button fx:id="addStandingOrder" mnemonicParsing="false" prefHeight="30.0" prefWidth="105.0" text="Standing Order">
                     <HBox.margin>
                        <Insets bottom="15.0" left="40.0" right="15.0" top="10.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="2.0" left="5.0" />
                     </padding>
                  </Button>
               </children></HBox>
            <VBox fx:id="addCategory" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="503.0" text="Sum">
                     <VBox.margin>
                        <Insets left="150.0" right="150.0" />
                     </VBox.margin>
                     <padding>
                        <Insets left="90.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="addSum" prefHeight="25.0" prefWidth="421.0">
                     <VBox.margin>
                        <Insets left="150.0" right="150.0" />
                     </VBox.margin>
                  </TextField>
                  <Label prefHeight="17.0" prefWidth="324.0" text="Category">
                     <VBox.margin>
                        <Insets left="150.0" right="150.0" top="10.0" />
                     </VBox.margin>
                     <padding>
                        <Insets left="75.0" />
                     </padding>
                  </Label>
                  <ChoiceBox prefHeight="25.0" prefWidth="534.0">
                     <VBox.margin>
                        <Insets left="150.0" right="150.0" />
                     </VBox.margin>
                  </ChoiceBox>
                  <Label prefHeight="17.0" prefWidth="667.0" text="Date">
                     <VBox.margin>
                        <Insets left="150.0" right="150.0" top="10.0" />
                     </VBox.margin>
                     <padding>
                        <Insets left="87.0" />
                     </padding>
                  </Label>
                  <DatePicker fx:id="addDate" prefHeight="25.0" prefWidth="571.0">
                     <VBox.margin>
                        <Insets left="150.0" right="150.0" />
                     </VBox.margin>
                  </DatePicker>
                  <Label prefHeight="17.0" prefWidth="508.0" text="Description">
                     <VBox.margin>
                        <Insets left="150.0" right="150.0" top="10.0" />
                     </VBox.margin>
                     <padding>
                        <Insets left="75.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="addDescription">
                     <VBox.margin>
                        <Insets left="150.0" right="150.0" />
                     </VBox.margin>
                  </TextField>
               </children></VBox>
            <VBox prefHeight="57.0" prefWidth="500.0">
               <children>
                  <Button fx:id="addAdd" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="32.0" prefWidth="88.0" text="Add">
                     <VBox.margin>
                        <Insets bottom="15.0" left="205.0" right="15.0" top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children></VBox>
    </center>

</BorderPane>
